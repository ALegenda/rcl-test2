    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.2",
    "@types/node-sass": "^4.11.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-infinite-scroller": "^1.2.3",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "env-cmd": "^10.1.0",
    "node-sass": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroller": "^1.2.6",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
